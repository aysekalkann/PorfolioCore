@model List<Service>
@{
    ViewData["Title"] = "ServiceList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2> Hizmetlerim</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hizmet</th>
        <th>Açıklama</th>
        <th>Durum</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.ServiceId</td>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td>@item.Status</td>
        </tr>
    }
</table>

