@model List<About>
@{
    ViewData["Title"] = "AboutList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2> Hakkımda</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>Unvan</th>
        <th>Yaş</th>
        <th>Açıklama</th>
        <th>Telefon</th>
        <th>Şehir</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>@item.NameSurname</td>
            <td>@item.Title</td>
            <td>@item.Age</td>
            <td>@item.Description</td>
            <td>@item.Phone</td>
            <td>@item.City</td>
        </tr>
    }
</table>

